[flake8]
ignore = A005,C101,D403,F403,F405,I100,I201,W503,W504,Q000
exclude =
    .git,
    __pycache__,
    venv
    migrations
filename = *.py
accept-encodings = utf-8
inline-quotes = single
max-complexity = 12
max-linenumber = 500
max-line-length = 120
multiline-quotes = double
per-file-ignores =
    myportfolio/myportfolio/settings/template/settings.dev.py:F821
    myportfolio/myportfolio/settings/template/settings.unittests.py:F821
